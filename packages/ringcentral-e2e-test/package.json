{
  "name": "ringcentral-e2e-test",
  "version": "0.0.1",
  "main": "index.js",
  "description": "E2E Tests for RingCentral Integration Apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/ringcentral-js-widget.git"
  },
  "scripts": {
    "testExample": "e2e-test run ./src/features/widgets/meeting/ScheduleMeeting.spec.js --params '{ \"levels\": [\"p1\"],\"tags\": [[\"widgets\", {\"brands\":[\"rc\"]}],[\"salesforce\"]] }' -D -S --testerCLI '--clearCache' --drivers puppeteer,seleniumWebdriverFirefox",
    "test": "e2e-test run ./src/features/widgets/meeting/ScheduleMeeting.spec.js --params '{ \"levels\": [\"p1\"],\"tags\": [[\"widgets\", {\"brands\":[\"rc\"], \"levels\": [\"p0\"]}],[\"salesforce\",{\"brands\":[\"att\"]}]] }'",
    "createCase": "npx e2e-test create --service '{ \"list\": [{\"origin\":\"einstein\",\"caseID\": [961] }]}'"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "chromedriver": "^2.42.0",
    "einstein-sdk": "^1.0.11",
    "geckodriver": "^1.12.2",
    "jest-html-reporter": "^2.4.2",
    "marten": "^1.0.0-alpha.1",
    "ramda": "^0.25.0",
    "request": "^2.88.0",
    "ringcentral-e2e-cli": "^0.0.1",
    "webphone-client": "^1.0.4"
  },
  "license": "MIT"
}